<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.TableController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <TableView fx:id="tableView" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="idColumn" prefWidth="58.0" text="ID" />
                <TableColumn fx:id="nameColumn" prefWidth="172.0" text="Name" />
                  <TableColumn fx:id="revenueColumn" minWidth="0.0" prefWidth="152.0" text="Revenue" />
              </columns>
            </TableView>
            <Button maxHeight="-Infinity" mnemonicParsing="false" onAction="#handleGoToScene1" prefHeight="75.0" prefWidth="610.0" text="Graph View" textAlignment="RIGHT">
               <font>
                  <Font size="40.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
