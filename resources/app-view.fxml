<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.GraphController">
   <children>
      <VBox maxHeight="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <children>
            <LineChart fx:id="lineChart" prefHeight="325.0" title="Profit">
              <xAxis>
                <CategoryAxis side="BOTTOM" fx:id="xAxis" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="yAxis" side="LEFT" />
              </yAxis>
            </LineChart>
            <Button maxHeight="-Infinity" mnemonicParsing="false" onAction="#handleGoToScene2" prefHeight="75.0" prefWidth="600.0" text="Table View" textAlignment="RIGHT">
               <font>
                  <Font size="40.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
